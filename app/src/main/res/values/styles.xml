<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="MediaBaseTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="themeActionBarBg">@color/colorPrimaryDark</item>
        <item name="themeListBg">@color/colorPrimary</item>
        <item name="themeCoverBg">#7DFFFF00</item>
        <item name="themeCancelIcon">@drawable/media_close_icon</item>
        <item name="themeCompleteTextUnSelectColor">#7D0000ff</item>
        <item name="themeCompleteTextSelectColor">#0000ff</item>
        <item name="themeCompleteBtnUnSelectBg">@drawable/finish_bg_df</item>
        <item name="themeCompleteTextSize">10sp</item>
        <item name="themeCompleteBtnSelectBg">@drawable/finish_bg_st</item>
        <item name="themeUnCheckDfIcon">@drawable/media_unselected</item>
        <item name="themeCheckDfIcon">@drawable/media_selected</item>
    </style>

</resources>