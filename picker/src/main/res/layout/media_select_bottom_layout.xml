<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#00000000">

    <TextView
        android:id="@+id/tv_media_select_preview"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:enabled="false"
        android:gravity="center"
        android:paddingStart="15dp"
        android:paddingEnd="15dp"
        android:text="预览"
        android:textColor="?attr/themeCompleteTextUnSelectColor"
        android:textSize="15sp"
        android:visibility="gone"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_media_select_finish"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:layout_marginBottom="40dp"
        android:background="?attr/themeCompleteBtnUnSelectBg"
        android:enabled="false"
        android:gravity="center"
        android:paddingStart="31dp"
        android:paddingTop="12dp"
        android:paddingEnd="31dp"
        android:paddingBottom="12dp"
        android:text="完成"
        android:textColor="?attr/themeCompleteTextUnSelectColor"
        android:textSize="?attr/themeCompleteTextSize"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>